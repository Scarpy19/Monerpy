{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@comps/*": [
        "src/components/*"
      ],
      "@svg/*": [
        "src/components/svgs/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@types/*": [
        "src/types/*"
      ],
      "@mid/*": [
        "src/middleware/*"
      ],
      "@prisma/*": [
        "prisma/*"
      ],
      "@lib/*": [
        "src/lib/*"
      ],
    }
  }
}